<svg width="800" height="450" viewBox="0 0 800 450" fill="none" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="800" height="450" fill="#1a1a1a"/>
  
  <!-- Gradient Definition -->
  <defs>
    <linearGradient id="projectGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#00d4ff;stop-opacity:0.6" />
      <stop offset="100%" style="stop-color:#a855f7;stop-opacity:0.6" />
    </linearGradient>
  </defs>
  
  <!-- Browser Window Mockup -->
  <rect x="50" y="80" width="700" height="300" rx="12" fill="#2a2a2a" stroke="url(#projectGradient)" stroke-width="2"/>
  
  <!-- Browser Header -->
  <rect x="50" y="80" width="700" height="40" rx="12" fill="#1a1a1a"/>
  <rect x="50" y="108" width="700" height="1" fill="url(#projectGradient)" opacity="0.3"/>
  
  <!-- Browser Dots -->
  <circle cx="75" cy="100" r="6" fill="#ff5f56"/>
  <circle cx="100" cy="100" r="6" fill="#ffbd2e"/>
  <circle cx="125" cy="100" r="6" fill="#27c93f"/>
  
  <!-- Ladybird Logo/Icon (Simplified Browser Icon) -->
  <g transform="translate(300, 180)">
    <circle cx="100" cy="50" r="60" fill="url(#projectGradient)" opacity="0.2"/>
    <path d="M70 50 Q100 20 130 50 Q100 80 70 50" fill="url(#projectGradient)"/>
    <circle cx="90" cy="45" r="5" fill="#1a1a1a"/>
    <circle cx="110" cy="45" r="5" fill="#1a1a1a"/>
  </g>
  
  <!-- Code Lines (Browser Content) -->
  <rect x="80" y="150" width="120" height="8" rx="4" fill="url(#projectGradient)" opacity="0.4"/>
  <rect x="80" y="170" width="180" height="8" rx="4" fill="#00d4ff" opacity="0.3"/>
  <rect x="80" y="190" width="90" height="8" rx="4" fill="#a855f7" opacity="0.3"/>
  
  <rect x="560" y="150" width="140" height="8" rx="4" fill="url(#projectGradient)" opacity="0.4"/>
  <rect x="560" y="170" width="100" height="8" rx="4" fill="#00d4ff" opacity="0.3"/>
  
  <!-- Decorative Elements -->
  <circle cx="100" cy="50" r="12" fill="#00d4ff" opacity="0.2"/>
  <circle cx="700" cy="400" r="15" fill="#a855f7" opacity="0.2"/>
  <circle cx="750" cy="50" r="10" fill="#00d4ff" opacity="0.2"/>
  
  <!-- Text -->
  <text x="400" y="420" font-family="'Poppins', sans-serif" font-size="24" font-weight="600" fill="url(#projectGradient)" text-anchor="middle">
    Ladybird Browser
  </text>
</svg>
